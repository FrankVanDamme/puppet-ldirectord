virtual=<%= @virtual %>:<%= @port %>
<% @real.each do |ip,config|
     config  = {} if !config
     port    = config['port'] || @port
     options = config['options'] || @real_options
-%>
    real=<%= ip %>:<%= port %> <%= options %>
<% end -%>
<% if @fallback -%>
<% port = @fallback_port || @port -%>
    fallback=<%= @fallback %>:<%= port %><% if @fallback_options %> <%= @fallback_options %><% end %>
<% end -%>
<% if @service -%>
    service=<%= @service %>
<% end -%>
<% if @request -%>
    request="<%= @request %>"
<% end -%>
<% if @receive -%>
    receive="<%= @receive %>"
<% end -%>
<% if @scheduler -%>
    scheduler=<%= @scheduler %>
<% end -%>
<% if @protocol -%>
    protocol=<%= @protocol %>
<% end -%>
<% if @checktype -%>
    checktype=<%= @checktype %>
<% end -%>
<% if @checkport -%>
    checkport=<%= @checkport %>
<% end -%>
<% if @checkcommand -%>
    checkcommand=<%= @checkcommand %>
<% end -%>
<% if @httpmethod -%>
    httpmethod=<%= @httpmethod %>
<% end -%>
<% if @virtualhost -%>
    virtualhost="<%= @virtualhost %>"
<% end -%>
<% if @login -%>
    login="<%= @login %>"
<% end -%>
<% if @passwd -%>
    passwd="<%= @passwd %>"
<% end -%>
<% if @database -%>
    database="<%= @database %>"
<% end -%>
<% if @netmask -%>
    netmask=<%= @netmask %>
<% end -%>
<% if @persistent -%>
    persistent=<%= @persistent %>
<% end -%>

